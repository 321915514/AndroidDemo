<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android" >

    <item android:id="@android:id/background">
        <shape>
            <corners android:radius="100dp" />
            <gradient
                android:startColor="#d4d4d4"
                android:centerColor="#d4d4d4"
                android:centerY="0.75"
                android:endColor="#d4d4d4"
                android:angle="270"
                />
        </shape>
    </item>

    <item android:id="@android:id/secondaryProgress">
        <clip>
            <shape>
                <corners android:radius="100dp" />
                <gradient
                    android:startColor="#FFB24E"
                    android:centerColor="#FFB24E"
                    android:centerY="0.75"
                    android:endColor="#FFB24E"
                    android:angle="270"
                    />
            </shape>
        </clip>
    </item>

    <item android:id="@android:id/progress">
        <clip>
            <shape>
                <corners android:radius="100dp" />
                <gradient
                    android:startColor="#FF9307"
                    android:centerColor="#FF9307"
                    android:centerY="0.75"
                    android:endColor="#FF9307"
                    android:angle="270"
                    />
            </shape>
        </clip>
    </item>

</layer-list>